on\r\n                        onClick={qtyCountClick}\r\n                        variant='outline-primary'\r\n                        className='qty-minus'\r\n                        id='qty-minus'\r\n                        disabled={qty === 0}\r\n                      >\r\n                        <i\r\n                          className='fa fa-minus'\r\n                          aria-hidden='true'\r\n                          id='qty-minus'\r\n                        ></i>\r\n                      </Button>\r\n                      <FormControl\r\n                        type='text'\r\n                        id='quantity'\r\n                        name='quantity'\r\n                        min='0'\r\n                        max={product.countInStock}\r\n                        onChange={handleChangeCount}\r\n                        value={validate(qty)}\r\n                      />\r\n                      <Button\r\n                        onClick={qtyCountClick}\r\n                        className='qty-plus'\r\n                        id='qty-plus'\r\n                        variant='outline-primary'\r\n                        disabled={qty === product.countInStock}\r\n                      >\r\n                        <i\r\n                          className='fa fa-plus'\r\n                          id='qty-plus'\r\n                          aria-hidden='true'\r\n                        ></i>\r\n                      </Button>\r\n                    </Form>\r\n                  ) : (\r\n                    <div>\r\n                      <h5>Out of Stock</h5>\r\n                    </div>\r\n                  )}\r\n                  <div className='my-3'>\r\n                    <Button\r\n                      variant='primary'\r\n                      size='sm'\r\n                      className='py-3'\r\n                      onClick={addToCartHandler}\r\n                      disabled={qty === 0}\r\n                    >\r\n                      <i\r\n   